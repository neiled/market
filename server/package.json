    "graphql": "^0.11.7",
    "jsonwebtoken": "^8.0.1",
    "kcors": "^2.2.1",
    "knex": "^0.13.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-json-error": "^3.1.2",
    "koa-jwt": "^3.2.2",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "pg": "^7.3.0"
