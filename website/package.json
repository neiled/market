{
  "name": "market-website",
  "version": "1.0.0",
  "description": "An incremental game",
  "author": "Neil Edwards <neil@plasticwater.com>",
  "private": true,
  "scripts": {
    "server-dev": "node build/dev-server.js",
    "server-test": "NODE_ENV=testing PORT=8081 API_URL=\"http://localhost:4000\" node build/dev-server.js",
    "start": "npm run dev",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "unit": "NODE_ENV=testing ./node_modules/.bin/jest",
    "unit-watch": "NODE_ENV=testing ./node_modules/.bin/jest --watch",
    "e2e": "NODE_ENV=testing ./node_modules/.bin/codeceptjs run --config test/e2e/codecept.json",
    "test": "npm run unit && npm run e2e"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.5.3",
    "jwt-decode": "^2.2.0",
    "vue": "^2.4.2",
    "vue-router": "^2.8.0",
    "vue-toasted": "^1.1.21",
    "vuelidate": "^0.5.0",
    "vuex": "^2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "codeceptjs-webdriverio": "^1.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-codeceptjs": "^0.2.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-jasmine": "^2.9.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.3.1",
    "eslint-plugin-vue": "^3.13.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "faker": "^4.1.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.2.0",
    "jest-vue": "^0.7.2",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "^1.0.0-beta.1",
    "wallaby-vue-compiler": "^1.0.2",
    "wallaby-webpack": "0.0.42",
    "wdio-selenium-standalone-service": "0.0.9",
    "webdriverio": "^4.8.0",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true
  }
}
